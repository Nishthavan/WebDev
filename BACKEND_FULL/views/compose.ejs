<%-include("partials/header")-%>
<h1>Compose</h1>
<form action="/compose" method="post">
  <div class="form-group">
    <label for="Title">Title</label>
    <input class="form-control" type="text" name="Cin_title">
  </div>
  <div class="form-group">
    <label for="Post">Post</label>
    <textarea class = "form-control"name="Cin_post" rows="8" cols="80"></textarea>
  </div>
  <button class="btn btn-primary btn-lg" type="submit" name="submit">Publish</button>
</form>

<%-include("partials/footer")-%>
